        <description>申請者、実申請者にログインユーザーをセットする</description>
        <field>ail_posted</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages>申請者、実申請者をセットしました。</messages>
        <name>Username</name>
        <order/>
        <script><![CDATA[function onLoad() {
    //Type appropriate comment here, and begin script below
    if (g_form.isNewRecord()) {
		alert('NewRecord');
        var username = g_user.userName;
        alert(username);
        g_form.setValue('applicant', username);
        g_form.setValue('real_applicant', username);
        // 新規は、申請区分を変更可能にする
        g_from.setReadonly('classification', false);
    } else {
		alert('EditRecord');
        // 新規以外は、申請区分を読取専用にする
        g_from.setReadonly('classification', true);

    }


}]]></script>
